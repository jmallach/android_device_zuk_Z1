[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Jordi Mallach (jmallach)
[access "refs/for/refs/*"]
	read = group user/Jordi Mallach (jmallach)
	push = group user/Jordi Mallach (jmallach)
	label-Code-Review = -2..+2 group user/Jordi Mallach (jmallach)
	label-Verified = -1..+1 group user/Jordi Mallach (jmallach)
	rebase = group user/Jordi Mallach (jmallach)
	removeReviewer = group user/Jordi Mallach (jmallach)
	submit = group user/Jordi Mallach (jmallach)
[access "refs/heads/*"]
	read = group user/Jordi Mallach (jmallach)
	create = group user/Jordi Mallach (jmallach)
	push = +force group user/Jordi Mallach (jmallach)
	pushMerge = group user/Jordi Mallach (jmallach)
[access "refs/tags/*"]
	push = group user/Jordi Mallach (jmallach)
	push = +force group user/Jordi Mallach (jmallach)
